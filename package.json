        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.46.4",
        "@typescript-eslint/parser": "^8.46.4",
        "dotenv": "^16.4.5",
        "dotenv-cli": "^7.4.4",
        "drizzle-kit": "^0.24.0",
        "eslint": "8.57.0",
        "jest": "^29.7.0",
        "jest-html-reporter": "^3.10.2",
        "msw": "^2.12.7",
