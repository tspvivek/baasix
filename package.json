{
    "name": "@tspvivek/baasix",
    "version": "0.1.0-alpha.17",
    "description": "Baasix - Backend-as-a-Service with 40+ tools for schema management, CRUD operations with 50+ filter operators, relationships, permissions, files, workflows, and more",
    "main": "dist/index.js",
    "type": "module",
    "exports": {
        ".": "./dist/index.js",
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "assets",
        "README.md",
        "LICENSE.MD"
    ],
    "scripts": {
        "start": "dotenv -e .env.production node server.prod.js",
        "development": "dotenv -e .env tsx server.js",
        "dev": "tsx watch server.js",
        "test": "NODE_OPTIONS='--experimental-vm-modules' jest --detectOpenHandles --forceExit --verbose",
        "testCoverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage --detectOpenHandles --forceExit",
        "testAll": "NODE_OPTIONS='--experimental-vm-modules' jest --runInBand --detectOpenHandles --forceExit",
        "build": "node build.js",
        "db:generate": "drizzle-kit generate",
        "db:push": "drizzle-kit push",
        "db:studio": "drizzle-kit studio",
        "db:migrate": "tsx drizzle/migrate.ts"
    },
    "author": "Vivek Palanisamy",
    "license": "LicenseRef-LICENSE.MD",
    "homepage": "https://www.baasix.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tspvivek/baasix.git"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.645.0",
        "@aws-sdk/s3-request-presigner": "^3.645.0",
        "@keyv/redis": "^2.8.5",
        "@socket.io/redis-adapter": "^8.3.0",
        "ai-digest": "^1.0.5",
        "argon2": "^0.41.1",
        "aws-sdk": "^2.1664.0",
        "axios": "^1.7.2",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "csv-parse": "^6.1.0",
        "dayjs": "^1.11.18",
        "drizzle-orm": "^0.33.0",
        "express": "^4.19.2",
        "express-fileupload": "^1.5.1",
        "express-rate-limit": "^7.5.0",
        "ffprobe": "^1.1.2",
        "ffprobe-static": "^3.1.0",
        "file-type": "^19.3.0",
        "ioredis": "^5.4.1",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "liquidjs": "^10.16.3",
        "lodash": "^4.17.21",
        "mime-types": "^2.1.35",
        "morgan": "^1.10.0",
        "node-schedule": "^2.1.1",
        "nodemailer": "^6.9.14",
        "pg": "^8.13.0",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.3",
        "postgres": "^3.4.4",
        "sharp": "^0.33.4",
        "short-unique-id": "^5.2.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-fileupload": "^1.5.1",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/mime-types": "^2.1.4",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.14.0",
        "@types/node-schedule": "^2.1.7",
        "@types/nodemailer": "^6.4.15",
        "@types/passport": "^1.0.16",
        "@types/passport-jwt": "^4.0.1",
        "@types/passport-local": "^1.0.38",
